# Paste command using clipboard
pyperclip.copy(run_command)
time.sleep(1)
pyautogui.hotkey('shift', 'insert')  # safer than ctrl+v in CMD
pyautogui.press('enter')
time.sleep(2)

# Paste password
pyperclip.copy(password)
time.sleep(1)
pyautogui.hotkey('shift', 'insert')
pyautogui.press('enter')

print("PowerShell script launched inside RDP session.")
