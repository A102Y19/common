def type_text_safely(text):
    for char in text:
        if char.isupper():
            keyboard.press_and_release('shift+' + char.lower())
        elif char == '_':
            keyboard.press_and_release('shift+-')
        else:
            keyboard.write(char)
        time.sleep(0.2)
    keyboard.press_and_release('enter')

for command in commands:
    if command == pass1:
        type_text_safely(command)
        time.sleep(0.2)
    else:
        pyperclip.copy(command)
        pyautogui.keyDown('ctrl')
        time.sleep(0.2)
        pyautogui.press('v')
        time.sleep(0.1)
        pyautogui.keyUp('ctrl')
        pyautogui.press('enter')
        time.sleep(4)
