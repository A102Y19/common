import os
import shutil

def transfer_executable_file(source_file, target_path):
    # Get the path to the user's Downloads folder dynamically
    user_downloads_path = os.path.join(os.path.expanduser("~"), "Downloads")
    
    # Combine the Downloads path with the file name passed in as input
    source_file_path = os.path.join(user_downloads_path, source_file)
    
    # Check if the source file exists before proceeding
    if not os.path.exists(source_file_path):
        print(f"Error: {source_file_path} not found.")
        return False
    
    # Create the destination file path
    destination_file = os.path.join(target_path, source_file)
    
    # Now, proceed with the file transfer using shutil.move
    try:
        print(f"Moving file {source_file_path} to {destination_file}...")
        
        # Moving the file
        shutil.move(source_file_path, destination_file)
        
        # Success
        print(f"File {source_file} moved successfully.")
        return True
    except Exception as e:
        print(f"Error moving file: {e}")
        return False
