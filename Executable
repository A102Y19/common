import zipfile
import os

def unzip_executable_file(zip_file_path, extract_to_path):
    try:
        if not zip_file_path.lower().endswith(".zip"):
            print(f"‚ùå Error: '{zip_file_path}' is not a zip file.")
            return False

        if not os.path.exists(zip_file_path):
            print(f"‚ùå Zip file not found: {zip_file_path}")
            return False

        if not os.path.exists(extract_to_path):
            os.makedirs(extract_to_path)

        print(f"üì¶ Unzipping '{zip_file_path}' to '{extract_to_path}'...")
        with zipfile.ZipFile(zip_file_path, 'r') as zip_ref:
            zip_ref.extractall(extract_to_path)

        print("‚úÖ Unzip completed successfully.")
        return True

    except Exception as e:
        print(f"‚ùå Failed to unzip '{zip_file_path}' - {e}")
        return False
